{
	"type": "origins:multiple",
	"callback":
	{
		"type": "origins:action_on_callback",
		"entity_action_added":
		{
			"type": "origins:execute_command",
			"command": "team join eggmilksoup @s"
		},
		"entity_action_lost":
		{
			"type": "origins:execute_command",
			"command": "team leave @s"
		}
	},
	"timer-val":
	{
		"type": "origins:resource",
		"min": 0,
		"max": 300,
		"hud_render":
		{
			"should_render": false
		}
	},
	"timer":
	{
		"type": "origins:action_over_time",
		"interval": 20,
		"entity_action":
		{
			"type": "origins:change_resource",
			"resource": "eggmilksoup:friend_timer-val",
			"change": 1
		}
	},
	"revoke":
	{
		"type": "origins:action_over_time",
		"entity_action":
		{
			"type": "origins:revoke_power",
			"power": "eggmilksoup:friend",
			"source": "eggmilksoup:eggmilksoup"
		},
		"condition":
		{
			"type": "origins:resource",
			"resource": "eggmilksoup:friend_cooldown",
			"comparison": "==",
			"compare_to": 300
		},
		"interval": 20
	},
	"name": "Friend of Egg Milk Soup",
	"description": "Egg Milk Soup's spawned mobs won't attack you"
}
